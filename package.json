{
    "name": "reglab-test",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint --fix",
        "prepare": "cd .. && husky install ./reglab-test/.husky",
        "postinstall": "npm run prepare",
        "coverage": "ng test --watch=false --code-coverage",
        "coverage-ci": "ng test --watch=false --code-coverage --browsers ChromeHeadless",
        "host": "ng serve --host 0.0.0.0",
        "compodoc:build": "compodoc -p tsconfig.doc.json",
        "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
        "compodoc:serve": "compodoc -s",
        "mock:server": "json-server --watch db.json",
        "start:server": "npx json-server db.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.1.0",
        "@angular/common": "^18.1.0",
        "@angular/compiler": "^18.1.0",
        "@angular/core": "^18.1.0",
        "@angular/forms": "^18.1.0",
        "@angular/platform-browser": "^18.1.0",
        "@angular/platform-browser-dynamic": "^18.1.0",
        "@angular/router": "^18.1.0",
        "@eslint/js": "^9.5.0",
        "@ngrx/component-store": "^18.0.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "json-server": "^1.0.0-beta.2",
        "lodash": "^4.17.21",
        "ngx-infinite-scroll": "^18.0.0",
        "prettier-eslint": "^16.3.0",
        "primeflex": "^3.3.1",
        "primeicons": "^7.0.0",
        "primeng": "^17.18.5",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.1.2",
        "@angular-eslint/builder": "^18.1.0",
        "@angular-eslint/eslint-plugin": "^18.1.0",
        "@angular-eslint/eslint-plugin-template": "18.1.0",
        "@angular-eslint/schematics": "^18.1.0",
        "@angular-eslint/template-parser": "^18.1.0",
        "@angular/cli": "^18.1.2",
        "@angular/compiler-cli": "^18.1.0",
        "@compodoc/compodoc": "^1.1.25",
        "@types/jasmine": "~5.1.0",
        "@types/lodash": "^4.17.7",
        "@typescript-eslint/eslint-plugin": "^7.14.1",
        "@typescript-eslint/parser": "^7.14.1",
        "angular-eslint": "18.1.0",
        "eslint": "^8.57.0",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "lint-staged": "^15.2.7",
        "ng-lint-staged": "^12.0.4",
        "prettier": "^3.3.2",
        "typescript": "~5.5.2",
        "typescript-eslint": "8.0.0-alpha.20"
    }
}
